class OrangeRickyFigure : Figure 
{
  public OrangeRickyFigure()
  {
    points = new int[2,4]
    {
      {0,0,0,1},
      {0,1,1,1}
    };
  }
}

class RhodeIslandZ : Figure 
{
  public RhodeIslandZ()
  {
    points = new int[2,4]
    {
      {0,0,1,1},
      {0,1,1,0}
    };
  }
}

class SmashboyFigure : Figure 
{
  public SmashboyFigure()
  {
    points = new int[2,4]
    {
      {0,0,1,1},
      {0,0,1,1}
    };
  }
}
