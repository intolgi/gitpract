namespace Calcul
{
	class Realnum
	{
		public delegate double Operation (double x, double y);

		public static Operation sum = (x,y) => x + y;
		public static Operation sun = (x,y) => x - y;
		public static Operation sup = (x,y) => x * y;

		public static double div (double x, double y);
		{
			if (y==0)
			{
				throw new Exeption();
			}
			return x/y;
		}
	}
	//добавление
}
