class BlueRickyFigure : Figure 
{
  public BlueRickyFigure()
  {
    points = new int[2,4]
    {
      {0,1,0,0},
      {0,1,1,1}
    };
  }
}

class ClevelandZFigure : Figure
{
  public ClevelandZFigure()
  {
    points = new int[2,4]
    {
      {0,1,1,0},
      {0,0,1,1}
    };
  }
}

class HeroFigure : Figure
{
  public HeroFigure()
  {
  points = new int[2,4]
    {
      {0,0,0,0},
      {1,1,1,1}
    };
  }
}
